import "../base.api"

type (
    // The data of upgrade lnx information | UpgradeLnx信息
    UpgradeLnxInfo {
        Id        *int    `json:"id,optional"`

        // 应用唯一标识
        Key  *string `json:"key,optional"`

        // 应用名称
        Name  *string `json:"name,optional"`

        // 应用包名
        PackageName  *string `json:"packageName,optional"`

        // 描述信息
        Description  *string `json:"description,optional"`

        // 是否删除 0：正常；1：已删除
        IsDel  *int32 `json:"isDel,optional"`

        // 创建时间
        CreateAt  *int64 `json:"createAt,optional"`

        // 修改时间
        UpdateAt  *int64 `json:"updateAt,optional"`
    }

        // The response data of upgrade lnx list | UpgradeLnx信息列表数据
    UpgradeLnxListResp {
        BaseDataInfo

        // The upgrade lnx list data | UpgradeLnx信息列表数据
        Data UpgradeLnxListInfo `json:"data"`
    }

        // The upgrade lnx list data | UpgradeLnx信息列表数据
    UpgradeLnxListInfo {
        BaseListInfo

        // The upgrade lnx list data | UpgradeLnx信息列表数据
        Data  []UpgradeLnxInfo  `json:"data"`
    }

        // Get upgrade lnx list request params | UpgradeLnx信息列表请求参数
    UpgradeLnxListReq {
        PageInfo

        // 应用唯一标识
        Key  *string `json:"key,optional"`

        // 应用名称
        Name  *string `json:"name,optional"`

        // 应用包名
        PackageName  *string `json:"packageName,optional"`

        // 描述信息
        Description  *string `json:"description,optional"`

        // 是否删除 0：正常；1：已删除
        IsDel  *int32 `json:"isDel,optional"`

        // 创建时间
        CreateAt  *int64 `json:"createAt,optional"`

        // 修改时间
        UpdateAt  *int64 `json:"updateAt,optional"`
    }

        // The upgrade lnx information response | UpgradeLnx信息返回体
    UpgradeLnxInfoResp {
        BaseDataInfo

        // upgrade lnx information | UpgradeLnx信息数据
        Data UpgradeLnxInfo `json:"data"`
    }
)

@server(
    jwt: Auth
    group: upgrade_lnx
    middleware: Authority
)

service Upgrade {
    // Create upgrade lnx information | 创建UpgradeLnx信息
    @handler createUpgradeLnx
    post /upgrade_lnx/create (UpgradeLnxInfo) returns (BaseMsgResp)

    // Update upgrade lnx information | 更新UpgradeLnx信息
    @handler updateUpgradeLnx
    post /upgrade_lnx/update (UpgradeLnxInfo) returns (BaseMsgResp)

    // Delete upgrade lnx information | 删除UpgradeLnx信息
    @handler deleteUpgradeLnx
    post /upgrade_lnx/delete (IDsReq) returns (BaseMsgResp)

    // Get upgrade lnx list | 获取UpgradeLnx信息列表
    @handler getUpgradeLnxList
    post /upgrade_lnx/list (UpgradeLnxListReq) returns (UpgradeLnxListResp)

    // Get upgrade lnx by ID | 通过ID获取UpgradeLnx信息
    @handler getUpgradeLnxById
    post /upgrade_lnx (IDReq) returns (UpgradeLnxInfoResp)
}
