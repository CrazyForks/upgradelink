    // Update upgrade lnx version information | 更新UpgradeLnxVersion信息
    @handler updateUpgradeLnxVersion
    post /upgrade_lnx_version/update (UpgradeLnxVersionInfo) returns (BaseMsgResp)

    // Delete upgrade lnx version information | 删除UpgradeLnxVersion信息
    @handler deleteUpgradeLnxVersion
    post /upgrade_lnx_version/delete (IDsReq) returns (BaseMsgResp)

    // Get upgrade lnx version list | 获取UpgradeLnxVersion信息列表
    @handler getUpgradeLnxVersionList
    post /upgrade_lnx_version/list (UpgradeLnxVersionListReq) returns (UpgradeLnxVersionListResp)

    // Get upgrade lnx version by ID | 通过ID获取UpgradeLnxVersion信息
    @handler getUpgradeLnxVersionById
    post /upgrade_lnx_version (IDReq) returns (UpgradeLnxVersionInfoResp)
}
