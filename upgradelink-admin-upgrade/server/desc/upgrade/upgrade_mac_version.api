import "../base.api"

type (
    // The data of upgrade mac version information | UpgradeMacVersion信息
    UpgradeMacVersionInfo {
        Id        *int    `json:"id,optional"`

        // 应用ID
        MacId  *int `json:"macId,optional"`

        // 云文件id
        CloudFileId  *string `json:"cloudFileId,optional"`

        // 版本名
        VersionName  *string `json:"versionName,optional"`

        // 版本号
        VersionCode  *int `json:"versionCode,optional"`

        // 机器架构:x64、ia32 arm64
        Arch  *string `json:"arch,optional"`

        // 版本文件大小
        VersionFileSize  *string `json:"versionFileSize,optional"`

        // 描述信息
        Description  *string `json:"description,optional"`

        // 是否删除 0：正常；1：已删除
        IsDel  *int32 `json:"isDel,optional"`

        // 创建时间
        CreateAt  *int64 `json:"createAt,optional"`

        // 修改时间
        UpdateAt  *int64 `json:"updateAt,optional"`
    }

    RespUpgradeMacVersionInfo {
        Id   *int    `json:"id,optional"`

        // 安卓应用ID
        MacId  *int `json:"macId,optional"`

        // 文件应用名称
        MacName  *string `json:"macName,optional"`

        // 云文件id
        CloudFileId  *string `json:"cloudFileId,optional"`

        // 云文件名称
        CloudFileName  *string `json:"cloudFileName,optional"`

        // 版本名
        VersionName  *string `json:"versionName,optional"`

        // 版本号
        VersionCode  *int `json:"versionCode,optional"`

        // 机器架构:x64、ia32 arm64
        Arch  *string `json:"arch,optional"`

        // 版本文件大小
        VersionFileSize  *string `json:"versionFileSize,optional"`

        // 描述信息
        Description  *string `json:"description,optional,default="`

        // 是否删除 0：正常；1：已删除
        IsDel  *int32 `json:"isDel,optional"`

        // 创建时间
        CreateAt  *int64 `json:"createAt,optional"`

        // 修改时间
        UpdateAt  *int64 `json:"updateAt,optional"`
    }

        // The response data of upgrade mac version list | UpgradeMacVersion信息列表数据
    UpgradeMacVersionListResp {
        BaseDataInfo

        // The upgrade mac version list data | UpgradeMacVersion信息列表数据
        Data UpgradeMacVersionListInfo `json:"data"`
    }

        // The upgrade mac version list data | UpgradeMacVersion信息列表数据
    UpgradeMacVersionListInfo {
        BaseListInfo

        // The upgrade mac version list data | UpgradeMacVersion信息列表数据
        Data  []RespUpgradeMacVersionInfo  `json:"data"`
    }

        // Get upgrade mac version list request params | UpgradeMacVersion信息列表请求参数
    UpgradeMacVersionListReq {
        PageInfo

        // 安卓应用ID
        MacId  *int `json:"macId,optional"`

        // 云文件id
        CloudFileId  *string `json:"cloudFileId,optional"`

        // 版本名
        VersionName  *string `json:"versionName,optional"`

        // 版本号
        VersionCode  *int `json:"versionCode,optional"`

        // 机器架构:x64、ia32 arm64
        Arch  *string `json:"arch,optional"`
    
        // 描述信息
        Description  *string `json:"description,optional"`

        // 是否删除 0：正常；1：已删除
        IsDel  *int32 `json:"isDel,optional"`

        // 创建时间
        CreateAt  *int64 `json:"createAt,optional"`

        // 修改时间
        UpdateAt  *int64 `json:"updateAt,optional"`
    }

        // The upgrade mac version information response | UpgradeMacVersion信息返回体
    UpgradeMacVersionInfoResp {
        BaseDataInfo

        // upgrade mac version information | UpgradeMacVersion信息数据
        Data RespUpgradeMacVersionInfo `json:"data"`
    }
)

@server(
    jwt: Auth
    group: upgrade_mac_version
    middleware: Authority
)

service Upgrade {
    // Create upgrade mac version information | 创建UpgradeMacVersion信息
    @handler createUpgradeMacVersion
    post /upgrade_mac_version/create (UpgradeMacVersionInfo) returns (BaseMsgResp)

    // Update upgrade mac version information | 更新UpgradeMacVersion信息
    @handler updateUpgradeMacVersion
    post /upgrade_mac_version/update (UpgradeMacVersionInfo) returns (BaseMsgResp)

    // Delete upgrade mac version information | 删除UpgradeMacVersion信息
    @handler deleteUpgradeMacVersion
    post /upgrade_mac_version/delete (IDsReq) returns (BaseMsgResp)

    // Get upgrade mac version list | 获取UpgradeMacVersion信息列表
    @handler getUpgradeMacVersionList
    post /upgrade_mac_version/list (UpgradeMacVersionListReq) returns (UpgradeMacVersionListResp)

    // Get upgrade mac version by ID | 通过ID获取UpgradeMacVersion信息
    @handler getUpgradeMacVersionById
    post /upgrade_mac_version (IDReq) returns (UpgradeMacVersionInfoResp)
}
