syntax = "v1"

info(
    title: "base api"
    desc: "base api"
    author: "toolsetlink"
    email: "toolsetlink@163.com"
    version: "v1.0"
)

// The basic response with data | 基础带数据信息
type BaseDataInfo {
    // Error code | 错误代码
    Code int    `json:"code"`

    // Message | 提示信息
    Msg  string `json:"msg"`

    // TraceId | 请求跟踪id
    TraceId  string `json:"traceId"`

    // Docs | 提示文档
    Docs  string `json:"docs"`

    // Data | 数据
    Data string `json:"data"`
}
